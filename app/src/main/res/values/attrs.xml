<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DonutProgress">
        <attr name="donut_progress" format="float"/>
        <attr name="donut_max" format="integer"/>

        <attr name="donut_unfinished_color" format="color"/>
        <attr name="donut_finished_color" format="color"/>

        <attr name="donut_finished_stroke_width" format="dimension"/>
        <attr name="donut_unfinished_stroke_width" format="dimension"/>

        <attr name="donut_text_size" format="dimension"/>
        <attr name="donut_text_color" format="color"/>

        <attr name="donut_prefix_text" format="string"/>
        <attr name="donut_suffix_text" format="string"/>
        <attr name="donut_text" format="string"/>

        <attr name="donut_background_color" format="color"/>

        <attr name="donut_inner_bottom_text" format="string"/>
        <attr name="donut_inner_bottom_text_size" format="dimension"/>
        <attr name="donut_inner_bottom_text_color" format="color"/>

        <attr name="donut_circle_starting_degree" format="integer" />
        <attr name="donut_show_text" format="boolean"/>
        <attr name="donut_inner_drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="circleProgressStyle" format="reference"/>
        <attr name="donutProgressStyle" format="reference"/>
        <attr name="arcProgressStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MYLoader">
        <attr name="my_color" format="color"/>

    </declare-styleable>
    <declare-styleable name="GoogleMapView">
        <attr name="setMapType"   format="integer">
            <enum name="satellite" value="0" />
            <enum name="roadmap"   value="1" />
            <enum name="hybrid"    value="2" />
            <enum name="terrain"    value="3" />
        </attr>

        <attr name="setMapScale" format="integer">
            <enum name="low" value="1"/>
            <enum name="high" value="2"/>
        </attr>
        <attr name="setLatitude" format="float"/>
        <attr name="setLongitude" format="float"/>
        <attr name="setMapZoom"    format="integer"/>
        <attr name="setMapWidth" format="integer"/>
        <attr name="setMapHeight" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AdCircleProgress">
        <attr name="adpgb_progress" format="float"/>
        <attr name="adpgb_max" format="integer"/>

        <attr name="adpgb_unfinished_color" format="color"/>

        <attr name="adpgb_finished_color" format="color"/>

        <attr name="adpgb_gradient_color_one" format="color"/>
        <attr name="adpgb_gradient_color_two" format="color"/>

        <attr name="adpgb_finished_stroke_width" format="dimension"/>
        <attr name="adpgb_unfinished_stroke_width" format="dimension"/>

        <attr name="adpgb_text_size" format="dimension"/>
        <attr name="adpgb_text_color" format="color"/>
        <attr name="adpgb_text_font" format="reference"/>

        <attr name="adpgb_prefix_text" format="string"/>
        <attr name="adpgb_suffix_text" format="string"/>
        <attr name="adpgb_text" format="string"/>

        <attr name="adpgb_background_color" format="color"/>

        <attr name="adpgb_inner_bottom_text" format="string"/>
        <attr name="adpgb_inner_bottom_text_size" format="dimension"/>
        <attr name="adpgb_inner_bottom_text_color" format="color"/>

        <attr name="adpgb_circle_starting_degree" format="integer" />
        <attr name="adpgb_show_text" format="boolean"/>
        <attr name="adpgb_inner_drawable" format="reference"/>

    </declare-styleable>

    <declare-styleable name="BubbleLayout">
        <attr name="bl_arrowWidth" format="dimension|reference" />
        <attr name="bl_cornersRadius" format="dimension|reference" />
        <attr name="bl_arrowHeight" format="dimension|reference" />
        <attr name="bl_arrowPosition" format="dimension|reference" />
        <attr name="bl_bubbleColor" format="color|reference" />
        <attr name="bl_strokeWidth" format="dimension|reference" />
        <attr name="bl_strokeColor" format="color|reference" />

        <attr name="bl_arrowDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
            <enum name="left_center" value="4"/>
            <enum name="right_center" value="5"/>
            <enum name="top_center" value="6"/>
            <enum name="bottom_center" value="7"/>
            <enum name="top_right" value="8"/>
            <enum name="bottom_right" value="9"/>
        </attr>
    </declare-styleable>

</resources>